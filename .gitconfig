[user]
    name = Fredrik MÃ¶llerstrand
    email = fredrik@mollerstrand.se

[core]
    excludesfile = /Users/fredrik/.gitignore-global
    autocrlf = input
    pager = delta

[init]
    defaultBranch = main

[color]
    diff = auto
    status = auto
    branch = auto

[diff]
    algorithm = patience

[interactive]
    diffFilter = delta --color-only

[delta]
    navigate = true

[branch]
    autosetuprebase = always

[push]
    default = current

[pull]
    rebase = true

[fetch]
    prune = true

[rebase]
    autosquash = true
