[init]
	defaultBranch = main

[core]
	autocrlf = input
	pager = delta

[color]
	diff = auto
	status = auto
	branch = auto

[diff]
	algorithm = patience

[interactive]
	diffFilter = delta --color-only

[delta]
	navigate = true

[branch]
	autosetuprebase = always

[push]
	default = current

[pull]
	rebase = true

[fetch]
	prune = true

[rebase]
	autosquash = true
